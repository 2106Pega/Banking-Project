package com.revature;

import java.util.ArrayList;

import com.revature.models.BankAccount;
import com.revature.models.CustomerAccount;
import com.revature.presentation.BankUi;
import com.revature.presentation.BankUiImpl;
import com.revature.repository.BankDao;
import com.revature.repository.BankDaoImpl;

public class MainDriver {
	
	public static void main(String[] args) {
		
		BankDao bDao = new BankDaoImpl();
		
		CustomerAccount c = new CustomerAccount(0, "Bob", "Miller", "bobmiller", "kjoozi?9283");
		
		bDao.insertCustomerAccount(c);
		
		BankAccount b = new BankAccount(0, 0, 1);
		
		bDao.insertBankAccount(b);
		
		BankUi ui = new BankUiImpl();
		
		ui.requestLogin();
		
	}

}
